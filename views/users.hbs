<div class="container">

  <h1 class="text-center">{{pageTitle}}</h1>


  <table class="table">
    <thead>
      <tr>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
      </tr>
    </thead>
    <tbody>
      {{#each returnUser}}
      <tr>
        <td>{{this.first_name}}</td>
        <td>{{this.last_name}}</td>
        <td>{{this.email}}</td>
        <td>{{this.password}}</td>

      </tr>
      {{/each}}
    </tbody>
  </table>
  <a href="/users/new"><button type="button" class="btn btn-outline-secondary">Add new User</button></a>

</div>